@model IEnumerable<MvcNetCoreEF2.Models.Enfermo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Inscripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Apellido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Direccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaNac)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Inscripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaNac)
            </td>
            <td>
                    @Html.ActionLink("Edit", "Edit", new { inscripcion = item.Inscripcion }) |
                    @Html.ActionLink("Details", "Details", new { inscripcion = item.Inscripcion }) |
                    @Html.ActionLink("Delete", "Delete", new { inscripcion = item.Inscripcion })
                    @Html.ActionLink("DeleteRaw", "DeleteRaw", new { inscripcion = item.Inscripcion })

            </td>
        </tr>
}
    </tbody>
</table>
