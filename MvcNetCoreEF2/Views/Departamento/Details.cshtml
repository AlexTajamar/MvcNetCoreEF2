@model Departamento

<div class="container text-center">
    <h2>Detalles</h2>
    <table class="table table-dark table-bordered mt-3">
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Dirección</th>
     
            </tr>
        </thead>
        <tbody>
                <tr>
                    <td>@Model.Nombre</td>
                    <td>@Model.Localizacion</td>

                </tr>
        </tbody>
    </table>
    <a asp-action="Index" class="btn btn-primary">Volver</a>
</div>